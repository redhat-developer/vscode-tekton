    "git-fetch-pack": "^0.1.1",
    "git-transport-protocol": "^0.1.0",
    "hasha": "^5.0.0",
    "js-yaml": "^3.13.1",
    "mkdirp": "^0.5.1",
    "open": "^6.4.0",
    "pluralize": "^4.0.0",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "semver": "^6.3.0",
